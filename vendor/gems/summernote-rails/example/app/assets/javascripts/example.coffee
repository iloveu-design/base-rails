$ ->
  $('.summernote').summernote
    height: 280
    lang: 'ko-KR'
  $('.summernote-plugin').summernote
    height: 280
    toolbar: [
      [
        'style'
        [
          'bold'
          'italic'
          'underline'
          'clear'
        ]
      ]
      [
        'font'
        [ 'strikethrough' ]
      ]
      [
        'fontsize'
        [ 'fontsize' ]
      ]
      [
        'color'
        [ 'color' ]
      ]
      [
        'para'
        [
          'ul'
          'ol'
          'paragraph'
        ]
      ]
      [
        'height'
        [ 'height' ]
      ]
      [
        'table'
        [ 'table' ]
      ]
      [
        'chart'
        [ 'chart' ]
      ]
      [
        'insert'
        [ 
          'hello'
        ]
      ]
    ]
